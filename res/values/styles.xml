<resources>
    
    <style name="MinMinTheme" parent="Theme.Sherlock">
        <item name="icon_cancel">@drawable/ic_action_cancel</item>
        <item name="icon_search">@drawable/ic_action_search</item>
    </style>
    
    <style name="MinMinTheme.Light" parent="Theme.Sherlock.Light.DarkActionBar">
        <item name="icon_cancel">@drawable/ic_action_cancel_light</item>
        <item name="icon_search">@drawable/ic_action_search_light</item>
    </style>

    <style name="Theme.Transparent" parent="Theme.Sherlock.Light">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    
</resources>
